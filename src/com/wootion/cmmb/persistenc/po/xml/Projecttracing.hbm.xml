<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.wootion.cmmb.persistenc.po.bean.Projecttracing" table="Projecttracing">
		<id name="trackid" type="java.lang.String">
			<column name="trackid" length="32"/>
			<generator class="uuid.hex" />
		</id>
		<property name="proid" type="java.lang.String">
			<column name="proid" length="32"/>
		</property>
		<property name="proname" type="java.lang.String">
			<column name="proname" length="32"/>
		</property>
		<property name="pagedata" type="java.lang.String">
			<column name="pagedata" length="5000"/>
		</property>
	</class>
</hibernate-mapping>
